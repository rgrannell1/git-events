#
# bash completion support for guake-cl





_guake-cl() {

	local cur prev opts
	COMPREPLY=()
	cur="${COMP_WORDS[COMP_CWORD]}"
	prev="${COMP_WORDS[COMP_CWORD-1]}"

	opts+=' -d'
	opts+=' --directory'
	opts+=' -h'
	opts+=' --help'
	opts+=' --version'

	if [[ ${cur} == -* ]] ; then
	    COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
	    return 0
	fi

}




complete -F _guake-cl guake-cl
